{
  "manifest_version": 2,
  "name": "Protein Tracker",
  "description": "Tracks a user's protein",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "scripts": ["eventpage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ]
}
